{
  "name": "parcel-vue-ts",
  "version": "1.0.0",
  "description": "A minimum Vue.js & TypeScript app with Parcel Bundler.",
  "main": "lib/parcel-typescript-example.js",
  "scripts": {
    "dev": "parcel serve src/index.html",
    "build": "cross-env NODE_ENV=production parcel build src/index.html -d build --public-url .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "author": "Masonz <masonzone@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.1.1",
    "gh-pages": "^1.1.0",
    "node-sass": "^4.7.2",
    "parcel-plugin-vue": "^1.0.1",
    "typescript": "^2.6.2",
    "vue-template-compiler": "^2.5.10"
  },
  "dependencies": {
    "vue": "^2.5.10",
    "vue-class-component": "^6.1.1",
    "vue-property-decorator": "^6.0.0"
  }
}
