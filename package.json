{
  "name": "parcel-vue-ts",
  "version": "1.0.0",
  "description": "A minimum Vue.js & TypeScript app with Parcel Bundler.",
  "main": "index.ts",
  "scripts": {
    "dev": "parcel serve src/index.html -d dist --public-url /dist/ -o",
    "build": "cross-env NODE_ENV=production parcel build src/index.html -d build --public-url .",
    "clear": "rm -rf .cache/ && rm -rf dist/ && rm -rf build/",
    "test": "nyc ava",
    "predeploy": "npm run build"
  },
  "author": "Masonz <masonzone@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-env": "^3.2.4",
    "cross-env": "^5.1.3",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1",
    "parcel-bundler": "^1.2.1",
    "parcel-plugin-vue": "^1.4.0",
    "require-extension-hooks": "^0.3.2",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^0.4.1",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.2",
    "vue-template-compiler": "^2.5.13",
    "vue-test-utils": "^1.0.0-beta.8"
  },
  "dependencies": {
    "vue": "^2.5.13",
    "vue-class-component": "^6.1.2",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "./test/helpers/setup.js"
    ]
  }
}
